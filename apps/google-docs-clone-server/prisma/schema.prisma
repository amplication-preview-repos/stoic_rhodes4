datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Link {
  createdAt  DateTime            @default(now())
  id         String              @id @default(cuid())
  permission EnumLinkPermission?
  updatedAt  DateTime            @updatedAt
  url        String?
}

model Document {
  content   String?
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  owner     String?
  title     String?
  updatedAt DateTime @updatedAt
}

model User {
  createdAt DateTime @default(now())
  email     String?  @unique
  firstName String?
  id        String   @id @default(cuid())
  lastName  String?
  password  String
  roles     Json
  updatedAt DateTime @updatedAt
  username  String   @unique
}

enum EnumLinkPermission {
  Option1
}
